{
  "name": "jstn-site",
  "version": "1.0.0",
  "description": "public demo site for jstn",
  "main": "index.jsx",
  "scripts": {
    "test": "jest",
    "dev": "webpack-dev-server --mode development --inline --port 4449",
    "build:dev": "webpack --mode development ./src/index.jsx --output ./dist/app.js",
    "build:prod": "webpack --mode production ./src/index.jsx --output ./dist/app.js",
    "publish": "git subtree split --branch gh-pages --prefix dist/"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/tylerchr/jstn.git"
  },
  "keywords": [
    "jstn"
  ],
  "author": "",
  "license": "BSD-3-Clause",
  "bugs": {
    "url": "https://github.com/tylerchr/jstn/issues"
  },
  "homepage": "https://github.com/tylerchr/jstn#readme",
  "devDependencies": {
    "autoprefixer": "^8.6.3",
    "babel-core": "^6.26.3",
    "babel-eslint": "^8.2.3",
    "babel-jest": "^23.0.1",
    "babel-loader": "^7.1.4",
    "babel-preset-env": "^1.7.0",
    "babel-preset-react": "^6.24.1",
    "clean-webpack-plugin": "^0.1.19",
    "css-loader": "^0.28.11",
    "eslint": "^4.19.1",
    "eslint-loader": "^2.0.0",
    "eslint-plugin-react": "^7.9.1",
    "html-loader": "^0.5.5",
    "html-webpack-plugin": "^3.2.0",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^23.1.0",
    "less": "^3.0.4",
    "less-loader": "^4.1.0",
    "mini-css-extract-plugin": "^0.4.0",
    "postcss-loader": "^2.1.5",
    "react": "^16.4.1",
    "react-dom": "^16.4.1",
    "style-loader": "^0.21.0",
    "webpack": "^4.12.0",
    "webpack-cli": "^3.0.8",
    "webpack-dev-server": "^3.1.4"
  },
  "jest": {
    "moduleDirectories": [
      "node_modules",
      "src"
    ],
    "moduleFileExtensions": [
      "js",
      "jsx"
    ],
    "moduleNameMapper": {
      "\\.(css|less)$": "identity-obj-proxy"
    }
  }
}
